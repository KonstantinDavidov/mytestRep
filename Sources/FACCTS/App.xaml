<Application x:Class="FACCTS.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:FACCTS">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <local:ClientBootstrap x:Key="bootstrapper" />
                </ResourceDictionary>
                <ResourceDictionary Source="pack://application:,,,/Themes/UI.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Themes/DataGrid.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/FACCTS.Controls;component/Resources/Buttons.xaml" />
                <ResourceDictionary Source="pack://application:,,,/FACCTS.Controls;component/Resources/Grids.xaml" />
                <ResourceDictionary Source="pack://application:,,,/FACCTS.Controls;component/Resources/Borders.xaml" />
                <ResourceDictionary Source="pack://application:,,,/FACCTS.Controls;component/Resources/TextBlocks.xaml" />
                <ResourceDictionary Source="pack://application:,,,/FACCTS.Controls;component/Resources/ComboBoxes.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
