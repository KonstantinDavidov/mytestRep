            <div id="wrapper">
        <div id="header"></div>
        <div id="tabs-wrapper">
            <div id="main-tabs">
                <ul>
                    <li><a href="#court-staff">
                        <span class="ui-icon ui-icon-image"></span>
                        Court Staff</a></li>
                    <li><a href="#court-settings">
                        <span class="ui-icon ui-icon-image"></span>
                        Court Settings</a></li>
                    <div id="login-info">
                        <label>User Login:</label>
                        <span>smyles@occ.gov</span>
                        <a href="#">Log Out</a>
                    </div>
                </ul>
                <div id="court-staff">
