                            <table id="court-staff-table" class="clickable" cellspacing="0">
                                <thead class="ui-widget-header">
                                    <tr>
                                        <td>User ID</td>
                                        <td>Email</td>
                                        <td>User Role</td>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr data-user-id="1">
                                        <td>Sophia Myles</td>
                                        <td>smyles@occ.gov</td>
                                        <td>Administrator</td>
                                    </tr>
                                    <tr data-user-id="2">
                                        <td>Pascal Hutton</td>
                                        <td>phutton@occ.gov</td>
                                        <td>Administrator</td>
                                    </tr>
                                    <tr data-user-id="3">
                                        <td>Ray Stevenson</td>
                                        <td>rstevenson@occ.gov</td>
                                        <td>Judicial Officer</td>
                                    </tr>
                                    <tr  data-user-id="4" class="active">
                                        <td>Amy Smart</td>
                                        <td>asmart@occ.gov</td>
                                        <td>Administrator</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                        <fieldset>
                            <button id="new-court-staff">NEW</button>
                            <button id="remove-court-staff">REMOVE</button>
                        </fieldset>
                    </div>
                    <div class="right-side-wrapper">
                        <div class="right-side">
                            <div id="user-tabs">
                                <ul>
                                    <li><a href="#profile">Profile</a></li>
                                    <li><a href="#availability">Availability</a></li>
                                    <li><a href="#user-role">User Role</a></li>
                                </ul>
                                <div id="profile">
                                    <form id="profile-form">
                                        <div class="form-fields-block">
                                            <div class="form-field">
                                                <label for="first-name">First Name</label>
                                                <input type="text" id="first-name" name="first-name" value="Amy" disabled>
                                            </div>
                                            <div class="form-field">
                                                <label for="middle-name">Middle Name</label>
                                                <input type="text" id="middle-name" name="middle-name" value="Ann" disabled>
                                            </div>
                                            <div class="form-field">
                                                <label for="last-name">Last Name</label>
                                                <input type="text" id="last-name" name="last-name" value="Smart" disabled>
                                            </div>
                                        </div>
                                        <div class="form-fields-block">
                                            <div class="form-field">
                                                <label for="user-id">User Id</label>
                                                <input type="text" id="user-id" name="user-id" value="amys" disabled>
                                            </div>
                                            <div class="form-field">
                                                <label for="user-email">User Email</label>
                                                <input type="text" id="user-email" name="user-email" value="amys@occ.gov" disabled>
                                            </div>
                                            <div class="form-field">
                                                <label for="user-phone">User Phone</label>
                                                <input type="text" id="user-phone" name="user-phone" value="234-234-2345" disabled>
                                            </div>
                                            <div class="form-field checkbox-field">
                                                <input type="checkbox" id="clets-certified" name="clets-certified" value="1" disabled>
                                                <label for="clets-certified" class="important">CLETS Certified</label>
                                            </div>
                                            <div class="form-element">
                                                <button id="profile-update">Update</button>
                                            </div>
                                        </div>
                                        <hr>
                                        <div class="form-fields-block">
                                            <div class="form-field">
                                                <label>Picture</label>
                                                <img src="images/image.jpg">
                                            </div>
                                            <div class="form-element">
                                                <a href="#">Click to change</a>
                                            </div>
                                        </div>
                                    </form>

                                    <div id="dialog-form" title="Update user profile">
                                        <form id="update-profile-form">
                                            <div class="form-fields-block">
                                                <p>
                                                    <label for="update-first-name">First Name</label>
                                                    <input type="text" id="update-first-name" name="first-name" class="text ui-widget-content ui-corner-all" required />
                                                </p>
                                                <p>
                                                    <label for="update-middle-name">Middle Name</label>
                                                    <input type="text" id="update-middle-name" name="middle-name" class="text ui-widget-content ui-corner-all">
                                                </p>
                                                <p>
                                                    <label for="update-last-name">Last Name</label>
                                                    <input type="text" id="update-last-name" name="last-name" class="text ui-widget-content ui-corner-all" required />
                                                </p>
                                            </div>
                                            <div class="form-fields-block">
                                                <p>
                                                    <label for="update-user-id">User Id</label>
                                                    <input type="text" id="update-user-id" name="user-id" class="text ui-widget-content ui-corner-all" required>
                                                </p>
                                                <p>
                                                    <label for="update-user-email">User Email</label>
                                                    <input type="email" id="update-user-email" name="user-email" class="text ui-widget-content ui-corner-all" required>
                                                </p>
                                                <p>
                                                    <label for="update-user-phone">User Phone</label>
                                                    <input type="text" id="update-user-phone" name="user-phone" class="text ui-widget-content ui-corner-all" required>
                                                </p>
                                                <p class="checkbox-field">
                                                    <input type="checkbox" id="update-clets-certified" name="clets-certified" class="text ui-widget-content ui-corner-all">
                                                    <label for="update-clets-certified" class="important">CLETS Certified</label>
                                                </p>
                                            </div>
                                        </form>
                                    </div>
                                </div>

                                <div id="availability">
                                    <form id="availability-form">
                                        <div class="form-fields-block">
                                            <div class="form-element">
                                                <input type="checkbox" name="availability-available" id="availability-available" checked>
                                                <label for="availability-available">Available</label>
                                            </div>
                                        </div>
                                        <hr>
                                        <div class="form-fields-block">
                                            <div class="form-element">
                                                <label for="availability-substitute">Select Substitude</label>
                                            </div>
                                            <div class="form-element">
                                                <select name="availability-substitute" id="availability-substitute">
                                                    <option value="administrator">Administrator</option>
                                                    <option value="judicial-office" selected>Judicial Officer</option>
                                                    <option value="courtroom-clerk">Courtroom Clerk</option>
                                                    <option value="self-help-center">Self Help Center</option>
                                                    <option value="business-office">Business Office</option>
                                                </select>
                                            </div>
                                            <div class="form-element">
                                                <table id="substitute-users-table" class="clickable bordered">
                                                    <thead class="ui-widget-header">
                                                        <tr>
                                                            <td>Substitute</td>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr>
                                                            <td data-user-id="1">Sophia Myles</td>
                                                        </tr>
                                                        <tr>
                                                            <td data-user-id="2">Pascal Hilton</td>
                                                        </tr>
                                                        <tr>
                                                            <td data-user-id="3">Tom Smith</td>
                                                        </tr>
                                                        <tr>
                                                            <td data-user-id="4">Amy Smart</td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                        <div class="form-fields-block">
                                            <div class="form-element">
                                                <button id="availability-assign">Assign</button>
                                                <button id="availability-remove" class="important">Remove</button>
                                            </div>
                                        </div>
                                        <hr>
                                    </form>
                                </div>
                                <div id="user-role">
                                    <form id="user-role-form">
                                        <table class="user-role-table">
                                            <tr>
                                                <td class="td-user-role">
                                                    <input type="radio" name="user-role" id="administrator" value="administrator" checked>
                                                    <label for="administrator">Administrator</label>
                                                </td>
                                                <td>
                                                    <input type="checkbox" name="administrator-court-settings" id="administrator-court-settings" disabled checked>
                                                    <label for="administrator-court-settings">Court Settings</label>
                                                </td>
                                                <td>
                                                    <input type="checkbox" name="administrator-court-staff" id="administrator-court-staff" disabled checked>
