    <Property name="IsNoGuns" type="bool"/>
    <Property name="CAPROSEntrySection" type="CAPROSEntry"/>
    <Property name="NoServiceFeeSection" type="NoServiceFee"/>
    <Property name="IsExpire" type="bool"/>
    <Property name="OrdersEndDate" type="DateTime?"/>
    <Property name="OrdersEndTime" type="DateTime?"/>
    <Property name="IsPOSGeneral" type="bool?"/>
    <Property name="LawersFeeAndCourtCostsSection" type="LawersFeeAndCourtCosts"/>
    <Property name="IsOtherOrdersAttached" type="bool"/>
