  </Model>
  
<!--CH110-->
  <Model name="CH110ConductChoice">
    <Property name="IsEnabled" type="bool"/>
    <Property name="NoAbuse" type="bool"/>
    <Property name="NoContact" type="bool"/>
    <Property name="DontTryToLocate" type="bool"/>
    <Property name="CoversOtherProtectedPerson" type="bool"/>
    <Property name="Other" type="bool"/>
    <Property name="OtherDescription" type="string"/>
    <Property name="Service" type="ServiceReqState"/>
    <Property name="ConductState" type="OrderItemState"/>
  </Model>
  
  <Model name="CH110RestrainedPersonPayment">
